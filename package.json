{
  "name": "image2file",
  "version": "0.1.0",
  "packageManager": "pnpm@7.1.5",
  "description": "Simple image / svg / psd / file transform",
  "author": "MuRong <admin@imuboy.cn>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/murongg",
  "homepage": "https://github.com/murongg/image2file#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/murongg/image2file.git"
  },
  "bugs": {
    "url": "https://github.com/murongg/image2file/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@webtoon/psd": "^0.2.0"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@murongg/eslint-config": "^0.1.2",
    "@types/node": "^17.0.35",
    "bumpp": "^7.1.1",
    "eslint": "^8.16.0",
    "esno": "^0.16.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "pnpm": "^7.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.3",
    "unbuild": "^0.7.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.9"
  },
  "lint-staged": {
    "*": "pnpm run lint:fix"
  }
}
